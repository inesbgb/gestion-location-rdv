<form method="post" action="{{ path('app_categorie_admin_delete', {'id': categorie.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.id) }}">
    <button class="bg-red-500 text-white px-4 py-2 rounded hover:bg-red-600" type="submit">Supprimer</button>
</form>